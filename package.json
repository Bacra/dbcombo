{
  "name": "dbcombo-client",
  "version": "2.0.3",
  "description": "dbcombo browser client",
  "main": "src/clientkey.js",
  "scripts": {
    "prepublish": "npm ls & node ./test/build/browserify",
    "build": "node ./test/build/browserify",
    "bench": "karma start -- base benchmark",
    "bench-dev": "karma start -- dev benchmark",
    "lint": "eslint .",
    "test": "cross-env DEBUG=dbcombo-client* mocha test/test_*.js",
    "test-cov": "istanbul cover _mocha -- test/test_* --reporter dot",
    "test-e2e": "karma start",
    "test-e2e-dev": "karma start -- dev",
    "test-sauce": "cross-env DEBUG=dbcombo-client* karma start -- sauce",
    "test-travis": "istanbul cover _mocha --report lcovonly -- test/test_* --reporter dot"
  },
  "keywords": [
    "dbcombo",
    "combo",
    "browser",
    "seasjs"
  ],
  "author": "bacrawu <bacra.woo@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Bacra/node-dbcombo-client.git"
  },
  "bugs": {
    "url": "https://github.com/Bacra/node-dbcombo-client/issues"
  },
  "homepage": "https://github.com/Bacra/node-dbcombo-client#readme",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.1",
    "browserify": "^16.2.2",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "eslint": "^5.0.1",
    "eslint-config-brcjs": "^0.1.0",
    "expect.js": "^0.3.1",
    "extend": "^3.0.1",
    "istanbul": "^0.4.5",
    "karma": "^2.0.4",
    "karma-browserify": "^5.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "platform": "^1.3.5",
    "seajs": "^3.0.2",
    "watchify": "^3.11.0"
  }
}
